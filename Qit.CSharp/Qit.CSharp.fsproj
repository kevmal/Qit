<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <IsPackable>true</IsPackable>
    <Version>0.0.2-alpha</Version>
    <Authors>Kevin Malenfant</Authors>
    <Description>Quotation toolkit</Description>
    <PackageProjectUrl>https://github.com/kevmal/Qit</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kevmal/Qit</RepositoryUrl>
    <PackageTags>FSharp Quotations F#</PackageTags>
    <Copyright>Copyright 2019</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Library.fs" />
    <Compile Include="trynew.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="3.0.0" />
    <PackageReference Include="Microsoft.CodeAnalysis.Workspaces.Common" Version="3.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Qit\Qit.fsproj" />
  </ItemGroup>

</Project>
